MAP
  NAME "Map Warper"
  SIZE 256 256
  EXTENT -180 -90 180 90
  IMAGETYPE png
  #INTERLACE OFF
  UNITS DD

  PROJECTION
"init=epsg:4326"
  END

  WEB
    IMAGEPATH "/tmp"
    METADATA
      "wms_title"     "Map Warper WMS"
      "wms_srs"       "EPSG:4269 EPSG:4326 EPSG:900913"
      ows_enable_request "*"
    END
  END


 LAYER
    NAME '<%= map_original_layer_name %>'
    TYPE RASTER
    DATA '<%= map_original_filename %>'
    METADATA
      'wms_title' '<%= map_title %>. Original.'
    END
    STATUS ON
    TRANSPARENCY 100
    PROJECTION
        'init=epsg:4326'
    END
 END



  LAYER
    NAME '<%= map_layer_name %>'
    TYPE RASTER
    DATA '<%= map_filename %>'
    METADATA
      'wms_title' '<%= map_title %>'
    END
    STATUS ON
    TRANSPARENCY 100
    PROJECTION
        'init=epsg:4326'
    END
 END



  OUTPUTFORMAT
    NAME GEOTIFF
    DRIVER "GDAL/GTiff"
    MIMETYPE "image/tiff"
    IMAGEMODE "BYTE"
    EXTENSION "tif"
  END
END

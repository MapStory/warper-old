#[cache]
#type=GoogleDisk
#base=<%= RAILS_ROOT + "/public/tiles/" %>
<% @layers.each do | layer | %>
<% unless layer.maps.warped.empty? %>
[layer_<%= layer.id %>]
type=WMSLayer
url=<%="http://maps.nypl.org/warper-dev/layers/wms/#{layer.id.to_s}"%>?
layers=image
bbox=<%= layer.get_bounds %>
spherical_mercator=yes
tms_type=google

<% end %>
<% end %>

